{
  "name": "dacha-game-systems",
  "version": "0.0.4",
  "exports": {
    ".": {
      "require": "./build/index.js",
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./events": {
      "require": "./build/events/index.js",
      "import": "./build/events/index.js",
      "types": "./build/events/index.d.ts"
    },
    "./widgets": {
      "require": "./build/widgets/index.js",
      "import": "./build/widgets/index.js",
      "types": "./build/widgets/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/jest": "29.5.11",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/three": "0.150.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dacha": "0.15.3",
    "dacha-workbench": "0.0.27",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.27.5",
    "husky": "7.0.4",
    "jest": "29.7.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "ts-jest": "29.1.1",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "dacha": ">=0.15.3",
    "dacha-workbench": ">=0.0.27",
    "react": ">=18.1.0",
    "react-dom": ">=18.1.0"
  },
  "sideEffects": false,
  "files": [
    "build",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:esm",
    "build:clean": "rm -rf build",
    "build:esm": "tsc --project tsconfig.esm.json",
    "lint": "eslint .",
    "test": "jest",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "description": "Dacha is a JavaScript game engine for building games in browser.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michailRemmele/dacha-game-systems.git"
  },
  "author": "Mikhail Remmele",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michailRemmele/dacha-game-systems/issues"
  },
  "homepage": "https://github.com/michailRemmele/dacha-game-systems#readme"
}
